<%= render 'user_info', user:current_user %>

<div class="col-md-8 offset-md-1">
  <h2>Users</h2>
  <table class="table">
    <thead>
      <% @users.each do |user| %>
        <tr>
          <th><%= image_tag user.get_profile_image(80,80) %></th>
          <th><%= user.name %></th>
          <th><%= link_to 'Show', user_path(user.id) %></th>
        </tr>
      <% end %>
    </thead>
  </table>
</div>